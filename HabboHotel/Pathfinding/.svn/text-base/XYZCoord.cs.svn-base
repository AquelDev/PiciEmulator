using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Butterfly.HabboHotel.Pathfinding
{
    struct XYZCoord
    {
        internal int X;
        internal int Y;
        internal double Z;

        public XYZCoord(int x, int y, double z)
        {
            this.X = x;
            this.Y = y;
            this.Z = z;
        }
    }
}
