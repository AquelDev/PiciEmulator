using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Butterfly.HabboHotel.Groups
{
    class Group
    {
        internal int groupID;
        internal string groupDescription;
        internal string groupBadge;

        public Group(int groupID, string groupDesc, string groupBadge)
        {
            this.groupID = groupID;
            this.groupDescription = groupDesc;
            this.groupBadge = groupBadge;
        }
    }
}
